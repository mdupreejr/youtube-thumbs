{
  "name": "YouTube Thumbs Rating",
  "version": "4.1.1",
  "slug": "youtube_thumbs",
  "description": "Rate YouTube videos (üëç/üëé) for songs playing on your AppleTV through Home Assistant",
  "url": "https://github.com/mdupreejr/youtube-thumbs",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "ingress": true,
  "ingress_port": 21812,
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:21812]/",
  "init": false,
  "hassio_api": true,
  "homeassistant_api": true,
  "hassio_role": "homeassistant",
  "host_network": true,
  "map": ["addon_config:rw"],
  "options": {
    "media_player_entity": "media_player.your_appletv",
    "log_level": "INFO",
    "sqlite_web_host": "127.0.0.1",
    "sqlite_web_port": 8080,
    "force_quota_unlock": false,
    "not_found_cache_hours": 168,
    "search_max_results": 25,
    "search_max_candidates": 10,
    "debug_endpoints_enabled": false,
    "song_tracking_enabled": true,
    "song_tracking_poll_interval": 30
  },
  "schema": {
    "media_player_entity": "str",
    "log_level": "list(DEBUG|INFO|WARNING|ERROR|CRITICAL)?",
    "force_quota_unlock": "bool?",
    "not_found_cache_hours": "int(1,168)?",
    "search_max_results": "int(1,50)?",
    "search_max_candidates": "int(1,50)?",
    "debug_endpoints_enabled": "bool?",
    "song_tracking_enabled": "bool?",
    "song_tracking_poll_interval": "int(10,300)?"
  }
}
